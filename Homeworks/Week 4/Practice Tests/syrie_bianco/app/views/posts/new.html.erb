<h1>New Post</h1>


<form class="" action="<%= posts_url %>" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">

  <label>Title
    <input type="text" name="post[title]" value="<%= @post.title if @post %>">
  </label>

  <label>Body
    <input type="text" name="post[body]" value="<%= @post.body if @post %>">
  </label>

<input type="submit" name="" value="Create New Post">

</form>
